<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="ConsoleCustomAppender" target="SYSTEM_OUT">
      <CustomLayout type="type1"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="ConsoleCustomAppender"/>
    </Root>
  </Loggers>
</Configuration>
