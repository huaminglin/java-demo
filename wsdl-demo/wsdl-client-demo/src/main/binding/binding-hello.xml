<bindings
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    wsdlLocation="../wsdl/hello.wsdl"
    xmlns="http://java.sun.com/xml/ns/jaxws"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<enableWrapperStyle>false</enableWrapperStyle>
	<bindings node="wsdl:definitions/wsdl:portType[@name='HelloProvider']/wsdl:operation[@name='sayHello']">
		<method name="sayHi">
			<javadoc>Say hi.</javadoc>
		</method>
	</bindings>
	
	<bindings node="wsdl:definitions">
		<package name="huaminglin.demo.wsdl.client.services">
			<javadoc>SEI package</javadoc>
		</package>
	</bindings>
    
</bindings>
