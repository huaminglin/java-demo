<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  version="1.0" wsdlLocation="../wsdl/hello.wsdl"
>
  <jxb:bindings node="/xsd:schema"
    schemaLocation="../wsdl/hello.xsd">
    <jxb:bindings node="//xsd:simpleType[@name='helloStatus']">
      <jxb:typesafeEnumClass>
        <jxb:typesafeEnumMember name="success"
          value="SUCCESS"/>
        <jxb:typesafeEnumMember name="failure"
          value="FAILURE"/>
      </jxb:typesafeEnumClass>
    </jxb:bindings>
  </jxb:bindings>
</jxb:bindings>